<dl>
  <dt>Product Title</dt>
  <dd><%= @product.title %></dd>
  <dt>Product Price</dt>
  <dd>$<%= @product.price %></dd>
  <dt>Product Description</dt>
  <dd><%= @product.description %></dd>
  <dt>Product Country</dt>
  <dd><%= @product.country_of_origin %></dd>
  <dt>Product Category</dt>
  <dd><%= @product.category.title %></dd>
  <dt>Difficulty Level</dt>
  <dd><%= @product.difficulty_level %></dd>
</dl>
<%= link_to 'Link to all products', products_path %>