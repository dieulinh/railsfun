<%= link_to 'New Product', new_product_path %>
<table>
	<tr>
		<th>
			Product Title
		</th>
		<th>
			Product Description
		</th>
		<th>
			Product Price
		</th>
		<th>
			Product Category
		</th>
		<th>
			Product Country
		</th>
		<th>
			Product Difficulty Level
		</th>
		<th>
			ACTIONS
		</th>
	</tr>
	
	<% @products.each do |p| %>
		<%= render partial: 'product', locals: { product: p} %>
		
	<% end %>

</table>
